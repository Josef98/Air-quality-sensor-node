    au32Config[1] = u32DFBA;            

     
    if (FMC_WriteConfig(au32Config, 2) < 0)
        return -1;                      

    

     
    ((SYS_T *) (((uint32_t)0x50000000) + 0x00000))->IPRST1 = (0x1ul << (0));
    return 0;                           
}

